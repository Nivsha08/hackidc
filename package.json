{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "dev-client": "PORT=3000 webpack-dev-server --host 0.0.0.0 --progress --debug --hot --colors --config ./build/webpack.dev.conf.js",
    "dev-server": "PORT=8080 ./node_modules/nodemon/bin/nodemon.js --exec babel-node ./src/server.js",
    "dev": "concurrently --kill-others --raw \"npm run dev-server\" \"npm run dev-client\"",
    "debug": "PORT=8080 ./node_modules/nodemon/bin/nodemon.js --inspect --exec babel-node ./src/server.js",
    "clean": "rm -rf dist && mkdir dist && npm run migrate-clean-temp",
    "build-server": "babel -d ./dist ./src -s --copy-files",
    "build-client": "node ./build/build.js",
    "build": "npm run clean && npm run build-server && npm run build-client && npm run migrate",
    "migrate": "./node_modules/.bin/sequelize db:migrate",
    "seed": "./node_modules/.bin/sequelize db:seed:all",
    "migrate-clean-temp": "./node_modules/.bin/sequelize db:migrate:undo:all",
    "postinstall": "npm run build",
    "start": "node ./dist/server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
